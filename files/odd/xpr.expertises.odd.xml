<?xml version="1.0" encoding="UTF-8"?>
<TEI xmlns:xi="http://www.w3.org/2001/XInclude" xmlns="http://www.tei-c.org/ns/1.0" xmlns:rng="http://relaxng.org/ns/structure/1.0">
  <teiHeader>
    <fileDesc>
      <titleStmt>
        <title>Title</title>
      </titleStmt>
      <publicationStmt>
        <p>Publication Information</p>
      </publicationStmt>
      <sourceDesc>
        <p>Information about the source</p>
      </sourceDesc>
    </fileDesc>
  </teiHeader>
  <text>
    <front>
      <div>
        
      </div>
    </front>
    <body>
      <schemaSpec ident="xprExpertisesModel" start="expertises expertise" ns="xpr" module="expertise">
        <elementSpec ident="expertises" module="expertise">
          <content>
            <elementRef key="expertise" minOccurs="1"/>
          </content>
        </elementSpec>
        <elementSpec ident="expertise" module="expertise">
          <content>
            <sequence>
              <elementRef key="meta"/>
              <elementRef key="control"/>
              <elementRef key="sourceDesc"/>
              <elementRef key="description"/>
            </sequence>
          </content>
        </elementSpec>
        <elementSpec ident="meta"/>
        <moduleSpec ident="expertise.control"/>
        <elementSpec ident="control" module="expertise.control">
          <content>
            <sequence>
              <elementRef key="publicationStatus"/>
              <elementRef key="localControl"/>
              <elementRef key="maintenanceHistory"/>
            </sequence>
          </content>
        </elementSpec>
        <elementSpec ident="maintenanceStatus">
          <valList type="closed">
            <valItem ident="created"/>
            <valItem ident="revised"/>
            <valItem ident="updated"/>
            <valItem ident="retroconverted"/>
          </valList>
        </elementSpec>
        <elementSpec ident="control" module="expertise.control">
          <content>
            <sequence>
              <elementRef key="maintenanceStatus"/>
              <elementRef key="publicationStatus"/>
              <elementRef key="localControl"/>
              <elementRef key="maintenanceHistory"/>
            </sequence>
          </content>
        </elementSpec>
        <elementSpec ident="maintenanceStatus" module="expertise.control">
          <valList type="closed">
            <valItem ident="created"/>
            <valItem ident="revised"/>
            <valItem ident="updated"/>
            <valItem ident="retroconverted"/>
          </valList>
        </elementSpec>
        <elementSpec ident="publicationStatus" module="expertise.control">
          <content>
            <textNode/>
          </content>
        </elementSpec>
        <elementSpec ident="localControl" module="expertise.control">
          <content>
            <elementRef key="term"/>
          </content>
        </elementSpec>
        <elementSpec ident="term" module="expertise.control">
          <valList>
            <valItem ident="in progress"/>
            <valItem ident="to revise"/>
            <valItem ident="completed"/>
          </valList>
        </elementSpec>
        <elementSpec ident="maintenanceHistory" module="expertise.control">
          <content>
            <elementRef key="maintenanceEvent" minOccurs="1"/>
          </content>
        </elementSpec>
        <elementSpec ident="maintenanceEvent" module="expertise.control">
          <content>
            <sequence>
              <elementRef key="eventType"/>
              <elementRef key="eventDateTime"/>
              <elementRef key="agentType"/>
              <elementRef key="agent"/>
              <elementRef key="eventDescription"/>
            </sequence>
          </content>
        </elementSpec>
        <elementSpec ident="eventType" module="expertise.control">
          <valList type="closed">
            <valItem ident="created"/>
            <valItem ident="revised"/>
            <valItem ident="updated"/>
            <valItem ident="retroconverted"/>
          </valList>
        </elementSpec>
        <elementSpec ident="eventDateTime" module="expertise.control">
          <content>
            <rng:text/>
          </content>
          <attList>
            <attDef ident="standardDateTime">
              <datatype>
                <rng:data type="dateTime">
                  <rng:param name="maxInclusive">2099-12-31T23:59:59</rng:param>
                </rng:data>
              </datatype>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="agentType" module="expertise.control">
          <valList type="closed">
            <valItem ident="human"/>
            <valItem ident="machine"/>
          </valList>
        </elementSpec>
        <elementSpec ident="agent" module="expertise.control">
          <content>
            <textNode/>
          </content>
        </elementSpec>
        <elementSpec ident="eventDescription" module="expertise.control">
          <content>
            <textNode/>
          </content>
        </elementSpec>
        <moduleSpec ident="expertise.sourceDesc"/>
        <elementSpec ident="sourceDesc" module="expertise.sourceDesc">
          <content>
            <elementRef key=""/>
            <macroRef key=""/>
          </content>
        </elementSpec>
        <elementSpec ident="description"></elementSpec>
        <macroSpec ident="expertise.idno.unitid">
          <content>
            <elementRef key="idno"/>
          </content>
        </macroSpec>
        <elementSpec ident="idno">
          <attList>
            <attDef ident="type">
              <valList>
                <valItem ident="unitid"/>
                <valItem ident="item"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
      </schemaSpec>
    </body>
  </text>
</TEI>

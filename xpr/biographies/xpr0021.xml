        <targetEntity targetType="person">
          <part localType="full">Philippe de Vitry</part>
          <part localType="status">Conseiller du roy, contrôleur général des rentes de l'Hôtel de cette ville de Paris</part>
          <part localType="occupationType">Autres;Autres</part>
          <part localType="age">42</part>
          <part localType="address">rue des Prouvaux, paroisse St Eustache</part>
          <part localType="origPlace"/>
          <part localType="xprRef"/>

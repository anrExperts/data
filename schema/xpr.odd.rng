   <define name="category">
      <element name="category">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Category) </a:documentation>
         <text/>
         <sch:ns xmlns="http://www.tei-c.org/ns/1.0"
                 xmlns:eg="http://www.tei-c.org/ns/Examples"
                 xmlns:math="http://www.w3.org/1998/Math/MathML"
                 xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:svg="http://www.w3.org/2000/svg"
                 xmlns:xi="http://www.w3.org/2001/XInclude"
                 xmlns:xpr="xpr"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                 prefix="xpr"
                 uri="xpr"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="xprOdd-category-categoryValues-constraint-rule-5">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:eg="http://www.tei-c.org/ns/Examples"
                      xmlns:math="http://www.w3.org/1998/Math/MathML"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:svg="http://www.w3.org/2000/svg"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns:xpr="xpr"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
                      context="xpr:category">
               <sch:let name="node" value="local-name()"/>
               <sch:let name="content" value="normalize-space(.)"/>
               <sch:let name="type" value="@type"/>
               <sch:let name="value">
                  <xsl:choose>
                     <xsl:when test="$type = 'assessment'">
                        <xsl:text>Décrire et évaluer les travaux à venir</xsl:text>
                     </xsl:when>
                     <xsl:when test="$type = 'settlement'">
                        <xsl:text>Départager</xsl:text>
                     </xsl:when>
                     <xsl:when test="$type = 'registration'">
                        <xsl:text>Enregistrer</xsl:text>
                     </xsl:when>
                     <xsl:when test="$type = 'estimation'">
                        <xsl:text>Estimer la valeur des biens</xsl:text>
                     </xsl:when>
                     <xsl:when test="$type = 'acceptation'">
                        <xsl:text>Recevoir et évaluer le travail réalisé</xsl:text>
                     </xsl:when>
                     <xsl:otherwise/>
                  </xsl:choose>
               </sch:let>
               <sch:assert test="$content = $value">Compte tenu du type sélectionné (<sch:value-of select="$type"/>), la valeur de l'élément &lt;<sch:value-of select="$node"/>&gt; doit correspondre à "<sch:value-of select="$value"/>".</sch:assert>
            </sch:rule>
         </pattern>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <choice>
               <value>assessment</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>settlement</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>registration</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>estimation</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <value>acceptation</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
